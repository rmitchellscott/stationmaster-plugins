<% if event_layout == 'week' %>
  <%= render partial: 'plugins/calendars/full_week', locals: local_assigns %>
<% elsif %w[month rolling_month].include?(event_layout) %>
  <%= render partial: 'plugins/calendars/full_month', locals: local_assigns %>
<% else %>
  <%= render partial: 'plugins/calendars/full_auto', locals: local_assigns %>
<% end %>
